    "uniswap",
    "workspaces",
    "yarn"
  ],
  "private": true,
  "scripts": {
    "react-app:build": "yarn workspace @mydex/react-app build --max-old-space-size=12288",
    "react-app:eject": "yarn workspace @mydex/react-app eject",
    "react-app:start": "yarn workspace @mydex/react-app start",
    "react-app:test": "yarn workspace @mydex/react-app test",
    "ship": "echo 'Sorry, this does not work yet'",
    "build": "yarn workspace @mydex/react-app build --max-old-space-size=12288",
    "chain": "yarn workspace @mydex/hardhat chain",
    "node": "cd hardhat && npx hardhat node",
    "test": "cd hardhat && npx hardhat test",
    "start": "yarn workspace @mydex/react-app start",
    "compile": "cd hardhat && npx hardhat compile",
    "deploy": "yarn workspace @mydex/hardhat deploy",
    "watch": "cd hardhat && node scripts/watch.js",
    "accounts": "cd hardhat && npx hardhat accounts",
    "balance": "cd hardhat && npx hardhat balance",
    "send": "cd hardhat && npx hardhat send"
  },
  "devDependencies": {},
  "workspaces": [ "*" ]
